<RelativeLayout android:id="@+id/RelativeLayout01" android:layout_width="fill_parent" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">



<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_alignParentLeft="true" android:id="@+id/tvJumpCount" android:text="Jump Count Goes Here"></TextView>
<EditText android:layout_height="wrap_content" android:layout_below="@+id/tvJumpCount" android:text="Aircraft" android:layout_width="fill_parent" android:id="@+id/etAircraft"></EditText><EditText android:layout_height="wrap_content" android:id="@+id/etLocation" android:layout_below="@+id/etAircraft" android:layout_width="fill_parent" android:text="Location"></EditText><TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_alignParentRight="true" android:layout_alignParentTop="true" android:text="Freefall Time Goes Here" android:id="@+id/tvFreefallTime"></TextView><LinearLayout android:id="@+id/LinearLayout01" android:layout_below="@+id/etLocation" android:layout_height="wrap_content" android:layout_width="fill_parent" android:gravity="fill_horizontal"><EditText android:layout_height="wrap_content" android:layout_below="@+id/etLocation" android:text="Altitude" android:id="@+id/etAltitude" android:layout_width="0px" android:layout_weight="1" android:inputType="number"></EditText><EditText android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_below="@+id/etLocation" android:layout_width="0px" android:layout_weight="1" android:inputType="number" android:id="@+id/etDelay" android:text="Delay"></EditText></LinearLayout><EditText android:id="@+id/etDescription" android:text="Description" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_below="@+id/LinearLayout01" android:inputType="textMultiLine" android:lines="8"></EditText><Button android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Log Jump" android:id="@+id/btnLogJump" android:layout_below="@+id/etDescription"></Button>










</RelativeLayout>
